                                <th style="text-align:center;" scope="col">
                                    ID
                                </th>
                                <th style="text-align:center;" scope="col">
                                    @Recursos.Idioma.Caixa
                                </th>
                                <th style="width:50%; text-align:center;" scope="col">
                                    @Recursos.Idioma.InfoAlarma
                                </th>
                                <th style="text-align:center;" scope="col">
                                    @Recursos.Idioma.Quantitat
                                </th>
                            </tr>
                        </thead>
                        <tbody style="background:white;">
                            @foreach (var producte in @Model.Strapex.Alarmes.GroupBy(e => e.AlarmaId))
                            {
                                <tr align="center">
                                    <td>@producte.Key</td>
                                    <td>@Model.Strapex.Alarmes.FirstOrDefault(e => e.AlarmaId == producte.Key).ProducteId</td>
                                    <td style="width:50%;">@Model.Strapex.Alarmes.FirstOrDefault(e => e.AlarmaId == producte.Key).Descripcio</td>

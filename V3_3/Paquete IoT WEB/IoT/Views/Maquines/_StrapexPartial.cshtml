                        <tr>
                            <th colspan="2" style="text-align: center;">@Recursos.Idioma.TotalMaquina</th>
                        </tr>
                    </thead>
                    <tbody style="background:white;">
                        <tr align="center">
                            <td>@Recursos.Idioma.TempsTotalFuncionament:</td>
                            <td>@Model.Strapex.DadesGeneriques.TempsTotalFuncionament</td>
                        </tr>
                        <tr align="center">
                            <td>@Recursos.Idioma.CanvisFormat: </td>
                            <td>@Model.Strapex.Estats.Where(e => e.EstatId == 5).Count()</td>

    </a>
    <p style="font-size:20px;">@Recursos.Idioma.InformacioGeneral</p>
    <div id="collapseInformacioGeneral" class="collapse show">
        <div class="row" id="divMagatzemCircularChartContainer" style="padding-bottom: 35px; padding-top: 20px; margin-top:20px;">
            <div class="col" id="CircularChartMagatzem" style="min-width: 300px; min-height: 300px;"></div>
            <div class="col">
                <div class="row" id="TableCaixesMagatzem" style="min-width: 300px; margin-top:2%; margin-right:5%">
                    <div class="col" style="padding: 20px;">
                        <table class="table" style="width: 100%; border-collapse: collapse; box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);">
                            <thead style="background-color: #2d2d2d; color: #ffffff;">
                                <tr>
                                    <th colspan="4" style="padding: 15px; text-align: center;">@Recursos.Idioma.CaixesTotalsPerMagatzem</th>
                                </tr>
                            </thead>
                            <tbody style="background: #3a3a3a; color: #e0e0e0;">
                                @for (int i = 1; i <= Model.Formadora.NumMagatzems; i++)
                                {
                                    if (i == 1 || i == 3)
                                    {
                                        <tr style="margin-bottom: 10px; border: 1px solid #444;">
                                            <td style="padding: 15px; vertical-align: middle;">@Recursos.Idioma.Magatzem @i:</td>
                                            <td style="padding: 15px; vertical-align: middle;">
                                                @if (Model.Formadora.Estats.Count() > 0)
                                                {
                                                    string magatzem = $"CaixesTotals_{i}";
                                                    int result = ((int)@Model.Formadora.Estats.Last().GetType().GetProperty(magatzem).GetValue(@Model.Formadora.Estats.Last(), null) - (int)@Model.Formadora.Estats.First().GetType().GetProperty(magatzem).GetValue(@Model.Formadora.Estats.First(), null));
                                                    @result
                                                }
                                                else
                                                {
                                                    int result = 0;
                                                    @result
                                                }
                                            </td>
                                        </tr>
                                    }
                                    else
                                    {
                                        <tr style="margin-bottom: 10px; border: 1px solid #444;">
                                            <td style="padding: 15px; vertical-align: middle;">@Recursos.Idioma.Magatzem @i:</td>
                                            <td style="padding: 15px; vertical-align: middle;">
                                                @if (Model.Formadora.Estats.Count() > 0)
                                                {
                                                    string magatzem = $"CaixesTotals_{i}";
                                                    int result = ((int)@Model.Formadora.Estats.Last().GetType().GetProperty(magatzem).GetValue(@Model.Formadora.Estats.Last(), null) - (int)@Model.Formadora.Estats.First().GetType().GetProperty(magatzem).GetValue(@Model.Formadora.Estats.First(), null));
                                                    @result
                                                }
                                                else
                                                {
                                                    int result = 0;
                                                    @result
                                                }
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
                <div id="EstaditiquesTaula" style="margin-top: 30px; margin-right:5%" class="row">
                    <div class="col" style="padding: 20px;">
                        <table class="table" style="width: 100%; border-collapse: collapse; box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);">
                            <thead style="background-color: #2d2d2d; color: #ffffff;">
                                <tr>
                                    <th colspan="4" style="padding: 15px; text-align: center;">@Recursos.Idioma.EstadistiquesGenerals</th>
                                </tr>
                            </thead>
                            <tbody style="background: #3a3a3a; color: #e0e0e0;">
                                <tr style="margin-bottom: 10px; border: 1px solid #444;">
                                    <td style="padding: 15px; vertical-align: middle;">@Recursos.Idioma.CadenciaMitja:</td>
                                    <td style="padding: 15px; vertical-align: middle;">@Model.Formadora.DadesGeneriques.CadenciaMitja</td>
                                    <td style="padding: 15px; vertical-align: middle;">@Recursos.Idioma.ProductesTotals:</td>
                                    <td style="padding: 15px; vertical-align: middle;">
                                        @if (Model.Formadora.Estats.Count() > 0)
                                        {
                                            double result = Model.Formadora.Estats.Last().CaixesTotals;
                                            @result
                                        }
                                        else
                                        {
                                            int result = 0;
                                            @result
                                        }
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
